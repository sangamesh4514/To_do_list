  <body>
      <h1 align='center'>Todo-List</h1>

     <div id="todo-table">
      <form>
         <input type='text' name='item' placeholder='add new item...' val=" " autocomplete="off" required />
         <button type="submit">Add item</button>

      </form>
      <ul>
        <% for(var i=0;i<todos.length ; i++){ %>
            <li><%=todos[i].item%></li>
        <%}%>
     </ul>

    </div>
